{
  "name": "paribu-case",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js",
    "test:api": "cucumber-js features/api --require step-definitions/api",
    "test:web": "cucumber-js features/web --require step-definitions/web",
    "test:web:all": "npm-run-all --parallel test:web:chromium test:web:firefox test:web:webkit",
    "test:web:chromium": "BROWSER=chromium npm run test:web",
    "test:web:firefox": "BROWSER=firefox npm run test:web",
    "test:web:webkit": "BROWSER=webkit npm run test:web",
    "test:web:edge": "BROWSER=edge npm run test:web",
    "report": "node support/generateHtmlReport.js",
    "generate-report": "node ./report-generator.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@cucumber/cucumber": "^12.0.0",
    "@playwright/test": "^1.54.1",
    "cucumber-html-reporter": "^7.2.0",
    "dotenv": "^17.2.0",
    "multiple-cucumber-html-reporter": "^3.9.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.54.1"
  }
}
